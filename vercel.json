{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/geocode",
      "dest": "/api/geocode.js"
    },
    {
      "src": "/callback_apple.html",
      "dest": "/api/callback_apple"
    },
    {
      "src": "/visualizer/(.*)",
      "dest": "api/index.py"
    }
  ]
}
